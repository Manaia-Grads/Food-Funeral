{
  "name": "food-funeral",
  "version": "2.0.0",
  "main": "server/index.js",
  "license": "MIT",
  "keywords": [
    "fullstack",
    "blog",
    "dark humour",
    "final-project"
  ],
  "scripts": {
    "start": "node server",
    "test": "jest",
    "test:watch": "jest --noStackTrace --watchAll",
    "dev": "run-p dev:client dev:server",
    "dev:client": "run-p webpack:watch tailwind",
    "dev:server": "nodemon --watch server server/index.js",
    "build": "run-p build:client build:server",
    "build:client": "npm run webpack -- --mode production",
    "build:server": "npm run knex -- migrate:latest",
    "lint": "eslint --ext .js,.jsx .",
    "tailwind": "tailwindcss -w -i ./client/tailwind.css -o ./server/public/styles.css",
    "webpack": "webpack --config ./client/webpack.config.js",
    "webpack:watch": "npm run webpack -- --watch",
    "knex": "knex --knexfile ./server/db/knexfile.js",
    "prettier": "prettier --write server client"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": "eda/react",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.11.0",
    "dotenv": "^16.0.2",
    "express": "^4.17.1",
    "express-jwt": "^7.7.5",
    "express-jwt-authz": "^2.4.1",
    "jwks-rsa": "^2.1.4",
    "knex": "^1.0.2",
    "multer": "^1.4.5-lts.1",
    "nock": "^13.2.9",
    "pg": "^8.5.1",
    "superagent": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@testing-library/dom": "^8.18.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "11.2.2",
    "@vscode/sqlite3": "^5.0.8",
    "babel-loader": "^8.2.1",
    "eslint": "^8.9.0",
    "eslint-config-eda": "^1.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^26.6.3",
    "nock": "^13.2.9",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^6.2.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "supertest": "6.0.1",
    "tailwindcss": "^3.0.24",
    "webpack": "5.9.0",
    "webpack-cli": "4.2.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
